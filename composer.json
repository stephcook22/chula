{
    "name": "chula/chula",
    "description": "A lightweight blogging framework based on markdown",
    "keywords": ["blog","cms"],
    "type" : "project",
    "homepage": "http://stephcook22.github.io/chula",
    "authors": [
        {
            "name": "Steph Cook",
            "homepage": "http://stephcook.co.uk"
        },
        {
            "name": "Qas Ashraf"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "silex/silex": "1.*",
        "filp/whoops": "1.*",
        "symfony/form": "~2.3",
        "symfony/twig-bridge": "~2.3",
        "symfony/validator": "~2.3",
        "symfony/config": "~2.3",
        "symfony/translation": "~2.3",
        "igorw/config-service-provider": "1.2.*@dev",
        "michelf/php-markdown": "1.3.*@dev",
        "symfony/security": "~2.3",
        "twig/extensions": "1.0.0"

    },
    "require-dev": {
        "codeception/codeception": "*",
        "codeception/phpbuiltinserver": "*",
        "squizlabs/php_codesniffer": "2.0.*@dev"
    },
    "autoload": {
        "psr-4": {
            "Chula\\": "src/"
        }
    }
}